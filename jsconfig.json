{
  "compilerOptions": {
    "checkJs": false,
    "types": ["@remix-run/cloudflare", "vite/client"],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "app/*"
      ]
    }
  },
  "include": [
    "./**/*.d.ts",
    "./**/*.js",
    "./**/*.jsx",
    "**/.server/**/*.js",
    "**/.server/**/*.jsx",
    "**/.client/**/*.js",
    "**/.client/**/*.jsx", "app/entry.server.tsx", "server.ts", "app/routes/_index.tsx"
  ],
  "noEmit": true
}